{"ast":null,"code":"import { EventBus } from \"@/main\";\nimport { mixins } from \"../../mixins\";\nimport editOptions from \"@/constants/editOptions\";\nexport default {\n  name: \"ActorEditDialog\",\n  mixins: [mixins],\n  props: {\n    actorName: {\n      type: String,\n      required: true\n    },\n    actorId: {\n      type: String,\n      required: true\n    },\n    showDialog: {\n      type: Boolean,\n      default: false\n    }\n  },\n  data: () => ({\n    options: {\n      profile: {\n        button: editOptions.profile,\n        icon: 'mdi-notebook-outline',\n        tip: 'Perfil - Habilidades - Redes'\n      },\n      experiences: {\n        button: editOptions.experiences,\n        icon: 'mdi-license',\n        tip: 'Exp. Profesional - Formaci칩n - Premios'\n      },\n      multimedia: {\n        button: editOptions.multimedia,\n        icon: 'mdi-camera-outline',\n        tip: 'Im치genes - Videos'\n      }\n    }\n  }),\n  methods: {\n    showEditForm(opt) {\n      EventBus.$emit('actorEditionForm_adminPanel_showEditForm', opt);\n    },\n    closeDialog() {\n      EventBus.$emit('actorEditionForm_adminPanel_showEditForm');\n    }\n  }\n};","map":{"version":3,"mappings":"AAuDA;AACA;AACA;AAEA;EACAA;EACAC;EACAC;IACAC;MACAC;MACAC;IACA;IACAC;MACAF;MACAC;IACA;IACAE;MACAH;MACAI;IACA;EACA;EACAC;IACAC;MACAC;QACAC;QACAC;QACAC;MACA;MACAC;QACAH;QACAC;QACAC;MACA;MACAE;QACAJ;QACAC;QACAC;MACA;IACA;EACA;EACAG;IACAC;MACAC;IACA;IACAC;MACAD;IACA;EACA;AACA","names":["name","mixins","props","actorName","type","required","actorId","showDialog","default","data","options","profile","button","icon","tip","experiences","multimedia","methods","showEditForm","EventBus","closeDialog"],"sourceRoot":"src/components","sources":["ActorEditDialog.vue"],"sourcesContent":["<template>\n  <v-row justify=\"center\">\n      <v-dialog\n          transition=\"fade-transition\"\n          v-model=\"showDialog\"\n          persistent\n          max-width=\"420px\"\n      >\n        <v-card tile>\n          <v-card-title>\n            <span class=\"text-h5\">Editar {{ actorName }}</span>\n          </v-card-title>\n          <v-card-text>\n            <v-col\n                v-for=\"(btn, btn_i) in options\"\n                :key=btn_i\n            >\n              <v-tooltip\n                bottom\n              >\n                <template v-slot:activator=\"{ on, attrs }\">\n                  <v-btn\n                    block\n                    tile\n                    outlined\n                    elevation=\"0\"\n                    x-large\n                    v-bind=\"attrs\"\n                    v-on=\"on\"\n                    @click=\"showEditForm(btn.button)\"\n                  >\n                    <v-icon class=\"mr-2\">{{ btn.icon }}</v-icon>\n                    {{ btn.button }}\n                  </v-btn>\n                </template>\n                <span>{{ btn.tip }}</span>\n              </v-tooltip>\n            </v-col>\n          </v-card-text>\n          <v-card-actions>\n            <v-spacer></v-spacer>\n            <v-btn\n                tile\n                text\n                @click=\"closeDialog\"\n            >\n              Cerrar\n            </v-btn>\n          </v-card-actions>\n        </v-card>\n      </v-dialog>\n  </v-row>\n</template>\n\n<script>\nimport {EventBus} from \"@/main\";\nimport {mixins} from \"../../mixins\";\nimport editOptions from \"@/constants/editOptions\";\n\nexport default {\n  name: \"ActorEditDialog\",\n  mixins: [mixins],\n  props: {\n    actorName: {\n      type: String,\n      required: true\n    },\n    actorId: {\n      type: String,\n      required: true\n    },\n    showDialog: {\n      type: Boolean,\n      default: false\n    }\n  },\n  data: () => ({\n    options: {\n      profile: {\n        button: editOptions.profile,\n        icon: 'mdi-notebook-outline',\n        tip: 'Perfil - Habilidades - Redes'\n      },\n      experiences: {\n        button: editOptions.experiences,\n        icon: 'mdi-license',\n        tip: 'Exp. Profesional - Formaci칩n - Premios'\n      },\n      multimedia: {\n        button: editOptions.multimedia,\n        icon: 'mdi-camera-outline',\n        tip: 'Im치genes - Videos'\n      }\n    }\n  }),\n  methods: {\n    showEditForm(opt){\n      EventBus.$emit('actorEditionForm_adminPanel_showEditForm',opt);\n    },\n    closeDialog(){\n      EventBus.$emit('actorEditionForm_adminPanel_showEditForm');\n    }\n  }\n}\n</script>\n\n<style scoped>\n\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}